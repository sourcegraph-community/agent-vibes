{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc -p . --noEmit",
    "typecheck:watch": "tsc -p . --noEmit --watch --preserveWatchOutput",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --cache",
    "check": "npm run typecheck && npm run lint",
    "fix": "npm run lint:fix",
    "check:fix": "npm run typecheck && npm run lint:fix",
    "rotate:supabase": "tsx scripts/rotate-supabase-secrets.ts",
    "replay:sentiments": "tsx scripts/replay-failed-sentiments.ts",
    "enqueue:backfill": "tsx scripts/enqueue-backfill.ts",
    "process:backfill": "tsx scripts/process-backfill.ts",
    "cleanup:raw-tweets": "tsx scripts/cleanup-old-raw-tweets.ts",
    "cleanup:sentiment-failures": "tsx scripts/cleanup-sentiment-failures.ts",
    "health-check": "tsx scripts/health-check.ts",
    "apply-migrations": "tsx scripts/apply-migrations.ts",
    "build:edge-functions": "tsx scripts/build-edge-functions.ts",
    "functions:serve": "npm run build:edge-functions && supabase functions serve sentiment-processor --env-file supabase/.env.local",
    "test:date-filtering": "tsx scripts/test-date-filtering.ts",
    "process:sentiments": "tsx scripts/process-sentiments.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "apify": "^3.4.5",
    "next": "15.5.2",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@stylistic/eslint-plugin": "^2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^5",
    "globals": "^15",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vitest": "^3.2.4"
  }
}
